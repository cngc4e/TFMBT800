{
  "name": "bt800",
  "version": "0.1.0",
  "description": "Contains source code bits for the Bt800 bot in Transformice.",
  "main": "dist/main.js",
  "scripts": {
    "start": "npm run tsc && npm run start:q",
    "start:q": "cross-env NODE_PATH=dist/ node -r dotenv/config .",
    "tsc": "rimraf dist && tsc --build",
    "tsc:watch": "rimraf dist && tsc --watch",
    "lint": "eslint -c .eslintrc --ext .ts ./src"
  },
  "license": "MIT",
  "dependencies": {
    "@cheeseformice/transformice.js": "^1.0.0-rc.2",
    "dotenv": "^10.0.0",
    "glob": "^7.2.0",
    "jsesc": "^3.0.2",
    "redis": "^4.0.0",
    "tiny-typed-emitter": "^2.1.0",
    "tslog": "^3.3.0"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/jsesc": "^3.0.1",
    "@types/redis": "^2.8.32",
    "cross-env": "^7.0.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.2"
  }
}
